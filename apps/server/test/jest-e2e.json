{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@raven-docs/db/(.*)$": "<rootDir>/../src/database/$1"
  },
  "setupFiles": ["<rootDir>/jest.setup.ts"]
}
